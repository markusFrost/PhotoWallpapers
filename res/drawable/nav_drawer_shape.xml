<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >

    <corners android:radius="0dp" />

    <gradient
        android:angle="270"
        android:centerColor="#CCFFFF"
        android:centerX="100%"
        android:endColor="#1C3D0D"
        android:startColor="#26FFCC"
        android:type="linear" />

    <padding
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp" />

    

    <stroke
        android:width="3dp"
      android:color="#356987" />

</shape>